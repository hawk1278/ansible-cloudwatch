[general]
state_file = /var/awslogs/state/agent-state

{% for log_stream in log_streams %}
[{{ log_stream }}]
datetime_format = %Y-%m-%d %H:%M:%S
file = {{ log_streams[log_stream].log_file }}
log_stream_name = {instance_id}
initial_position = {{ log_streams[log_stream].position }}
log_group_name = {{ log_streams[log_stream].group_name }}
buffer_duration = 5000

{% endfor %}
